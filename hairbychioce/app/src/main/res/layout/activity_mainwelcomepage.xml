<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/background"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/flowerblue"
    tools:context=".MainActivity_Welcome_Page">

    <TextView
        android:id="@+id/nameDisplay"
        android:layout_width="108dp"
        android:layout_height="41dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="104dp"
        android:text="TextView"
        android:textColor="#201E1E"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/ageDisplay"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome" />

    <TextView
        android:id="@+id/ageDisplay"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginTop="344dp"
        android:layout_marginEnd="84dp"
        android:text="TextView"
        android:textColor="#242020"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.093" />

    <TextView
        android:id="@+id/welcome"
        android:layout_width="173dp"
        android:layout_height="51dp"
        android:layout_marginStart="116dp"
        android:layout_marginBottom="100dp"
        android:shadowColor="#1B1919"
        android:text="Welcome"
        android:textColor="#0F0D0D"
        android:textColorHint="#1B1A1A"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/ageDisplay"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>